{
  "name": "deep-web-solutions/wp-framework-test-plugin",
  "type": "wordpress-plugin",

  "description": "A barebones WP plugin based on the DWS WP Framework used for running tests.",
  "keywords": ["wordpress", "framework", "Deep Web Solutions", "test plugin"],
  "homepage": "https://www.deep-web-solutions.de",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Antonius Hegyes",
      "email": "a.hegyes@deep-web-solutions.de",
      "homepage": "https://www.linkedin.com/in/tonyhegyes",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/Deep-Web-Solutions-GmbH/wordpress-framework-test-plugin/graphs/contributors"
    }
  ],

  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Deep-Web-Solutions-GmbH/wordpress-framework-core"
    },
    {
      "type": "vcs",
      "url": "https://github.com/Deep-Web-Solutions-GmbH/wordpress-framework-helpers"
    }
  ],
  "require": {
    "php": ">=7.4",
    "php-di/php-di": "~6.1.0",
    "composer/installers": "~1.0",
    "deep-web-solutions/wp-framework-core": "master@dev",
    "deep-web-solutions/wp-framework-helpers": "master@dev"
  },
  "require-dev": {
    "wpreadme2markdown/wp2md": "~3.0",

    "phpmd/phpmd": "*",
    "squizlabs/php_codesniffer": "^3.5",
    "dealerdirect/phpcodesniffer-composer-installer": "*",
    "object-calisthenics/phpcs-calisthenics-rules": "*",
    "phpcompatibility/php-compatibility": "*",
    "phpcompatibility/phpcompatibility-wp": "*",
    "wp-coding-standards/wpcs": "*"
  },
  "autoload": {
    "psr-4": {
      "DeepWebSolutions\\Plugins\\FrameworkTest\\": "/includes"
    }
  }
}